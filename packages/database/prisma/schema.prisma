generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model depts {
  dept String @id @db.VarChar(32)
}

model syllabi {
  courseid    String @db.VarChar(32)
  term        String @db.Char(7)
  instructors String @db.VarChar(255)
  link        String @db.VarChar(255)

  @@id([courseid, term, instructors])
  @@index([courseid], map: "courseidindex")
}
