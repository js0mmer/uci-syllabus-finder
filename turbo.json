{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^db:generate","^build"],
      "outputs": [".svelte-kit/**", ".vercel/**"]
    },
    "deploy": {
      "dependsOn": ["build", "test", "lint"]
    },
    "lint": {},
    "dev": {
      "dependsOn": ["^db:generate"],
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false
    },
    "db:pull": {
      "cache": false
    },
    "scrapers:test": {
      "cache": false
    }
  }
}